{
	"name": "React Vite",
	"dockerComposeFile": [
		"../../docker-compose.yml"
	],
	"service": "frontend",
	"workspaceFolder": "/workspace/frontend",
	"shutdownAction": "none",
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode",
				"dsznajder.es7-react-js-snippets"
			]
		}
	},
	"remoteUser": "node",
	"postCreateCommand": "npm install",
	"forwardPorts": [
		4010
	]
}